@model ProDat.Web2.Models.ETL.Import

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Create</h1>

<h4>Import</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post" enctype="multipart/form-data" asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="ImportTypeId" class="control-label"></label>
                <select asp-for="ImportTypeId" class="form-control" asp-items="ViewBag.ImportTypeId"></select>
            </div>

            <div class="form-group">
                <label asp-for="CreatedComment" class="control-label"></label>
                <input asp-for="CreatedComment" class="form-control" />
                <span asp-validation-for="CreatedComment" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="File" class="control-label"></label>
                <input asp-for="File" class="form-control-file" />
                <span asp-validation-for="File" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>

        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to Import Register</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
